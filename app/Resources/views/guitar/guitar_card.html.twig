{# app/Resources/views/guitar/guitar_card.html.twig #}
<div class = "thumbnail">
    <img src = {{ asset('images/' ~ guitar.imgPath) }} alt = {{ guitar.name }}>
    <div class = "caption">
        <h3>{{ guitar.name }}</h3>
        <p>{{ guitar.description }}</p>
        
        <p>
        <a href = "#" class = "btn btn-primary" role = "button">
            Rate
        </a>
        <a href = "#" class = "btn btn-default" role = "button">
            Add
        </a>
        </p>
    
    </div>
</div>

